{
    "builders": [
    {   
        "vm_name": "centos65",
        "type": "virtualbox-ovf",
        "source_path": "output-virtualbox-iso/centos65.ovf",
        "ssh_username": "root",
        "ssh_password": "root",
        "ssh_wait_timeout": "30m",
        "shutdown_command": "echo 'packer' | sudo -S shutdown -P now"
    }],

    "provisioners": [
    {
        "type": "shell",
        "script": "provision.sh"
    }],

    "post-processors": [
    {
        "type": "vagrant",
        "only": ["virtualbox-ovf"]

    }]
}
